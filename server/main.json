    "function fibonacci(n) {",
    "    let fib_series = [0, 1];",
    "    while (fib_series.length < n) {",
    "        fib_series.push(fib_series[fib_series.length - 1] + fib_series[fib_series.length - 2]);",
    "    }",
    "    return fib_series;",
    "}",
    "",
    "const result = fibonacci(10);",
    "console.log(result);"
  ]
}


